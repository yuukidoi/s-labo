
   <div class="card", id="new_image_document">
                <div class="card-header">
                 
                    <div class="view_box">
                        <%= f.file_field :image, class: "file" %>
                        <%= f.hidden_field :image_cache %>
                        <%= f.hidden_field :document_id, value: @document.id %>
                    </div>
                </div>
            <div class="card-body">
                    <%= f.label :image_comment, 'コメント'%>
                    <%= f.text_field :image_comment, class: 'form-control' %>
            </div>
        </div>

     
    
    
